# Google Places API Configuration
GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Yelp API Configuration (uses Spoonacular API backend)
# Optional - app will use generated menus if not provided
YELP_API_KEY=your-yelp-api-key

# Google Distance Matrix API Configuration
# Optional - app will use estimated delivery time if not provided
GOOGLE_DISTANCE_MATRIX_API_KEY=your-google-distance-matrix-api-key

# Firebase Configuration
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
FIREBASE_APP_ID=your-firebase-app-id

# Email Service Configuration (Optional)
# Resend API (Recommended)
RESEND_API_KEY=your-resend-api-key
RESEND_FROM_EMAIL=onboarding@resend.dev

# SendGrid API (Alternative)
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=test@example.com
